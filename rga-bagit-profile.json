{
	"name": "RGA BagIt Profile",
	"description": "BagIt Profile for the Rohingya Genocide Archive.",
	"acceptBagItVersion": [
		"0.97",
		"1.0"
	],
	"acceptSerialization": [
        "application/x-7z-compressed",
        "application/tar",
        "application/zip",
        "application/gzip",
        "application/x-rar-compressed",
        "application/tar+gzip"
	],
	"Allow-Fetch.txt": false,
   	"BagIt-Profile-Info":{
      		"BagIt-Profile-Identifier":"https://raw.githubusercontent.com/yvonneng/rga/main/rga-bagit-profile.json",
      		"BagIt-Profile-Version": "1.3.0",
     		"Source-Organization":"Rohingya Genocide Archive",
      		"Contact-Email":"rohingyaarchive@gmail.com",
     		"External-Description":"BagIt Profile for the Rohingya Genocide Archive. Last updated 9 August 2023.",
    		"Version":"1.0"
 	},
	"Manifests-Required":[
      		"md5"
  	],
	"Tag-Manifests-Allowed": [
		"md5",
		"sha1",
		"sha224",
		"sha256",
		"sha384",
		"sha512"
	],
	"Tag-Files-Required": [
  		  "md5"
	],
	"Tag-Manifests-Allowed": [
		"md5",
		"sha1",
		"sha224",
		"sha256",
		"sha384",
		"sha512"
	],
	"Tag-Files-Allowed": [
		"*"
	],
	"Bag-Info":{
   	   "Source-Organization":{
       		"required":true,
        	 "values":[
         	"Simon Fraser University",
          	  "York University"
         ]
      },
     	 "Organization-Address":{
        	 "required":true,
        	 "values":[
           	 "https://rohingyagenocidearchive.org/"
         ]
      },
     	 "Contact-Email":{
        	 "required":true,
		 "values":[
	 	   "rohingyaarchive@gmail.com"
	 ]
      },
    	  "External-Identifier":{
         "required":false
      },
     	 "Bag-Size":{
         "required":true
      },
    	  "Internal-Sender-Identifier":{
         "required":false
      },
     	 "Bagging-Date":{
         "required":true
      },
     	 "Payload-Oxum":{
         "required":true
      }
   },

	"serialization": "optional",
	"baseProfileId": null,
	"isBuiltIn": false,
	"tarDirMustMatchName": false
}
